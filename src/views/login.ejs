<div class="app-form-container login-form-container">
    <div ng-show="loginFailed">
        <p class="warning-msg">Incorrect username password</p>
    </div>
    <form name="loginForm" ng-submit="submit()">
        <div>
            <md-input-container>
                <label>User Name</label>
                <input type="text" ng-model="userName" required>
            </md-input-container>
            <md-input-container>
                <label>Password</label>
                <input type="password" ng-model="password" required>
            </md-input-container>
        </div>
        <div>
            <md-button id="login-btn" class="md-fab md-primary md-mini" type="submit">
                <i class="fa fa-sign-in"></i>
            </md-button>
        </div>
    </form>
    <p>New user?</p>
    <md-button class="submit-btn" class="submit-btn md-raised md-primary"  ng-click="navigateToSignUp()">Signup</md-button>
</div>